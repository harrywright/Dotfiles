[include]
	path = ~/user.gitconfig
[pull]
	rebase = true
[core]
	pager = /usr/local/share/git-core/contrib/diff-highlight/diff-highlight | less
[color]
	ui = always
[color "diff-highlight"]
 	oldNormal = red bold
 	oldHighlight = red bold 52
 	newNormal = green bold
 	newHighlight = green bold 22
[color "diff"]
 	commit = green
 	meta = yellow
 	frag = cyan
 	old = red
 	new = green
 	whitespace = red reverse
